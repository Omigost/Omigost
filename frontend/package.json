{
  "name": "react-webpack-typescript-starter",
  "version": "0.1.0",
  "description": "Starter kit for React, Webpack (with Hot Module Replacement), Typescript and Babel.",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "author": "Viktor Persson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vikpe/react-webpack-typescript-starter.git"
  },
  "bugs": {
    "url": "https://github.com/vikpe/react-webpack-typescript-starter/issues"
  },
  "homepage": "https://github.com/vikpe/react-webpack-typescript-starter",
  "scripts": {
    "build": "yarn run clean-dist && webpack -p --config=configs/webpack/prod.js && jest --coverage --config=configs/jest.json",
    "clean-dist": "rm -f -r -d dist",
    "lint": "tslint './src/**/*.ts*' --format stylish --force",
    "start": "yarn run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "start-prod": "yarn run build && node express.js"
  },
  "resolutions": {
    "@types/react": "^16.7.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.12",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.1",
    "express": "^4.16.4",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^23.6.0",
    "jest-html-reporter": "^2.4.2",
    "node-sass": "^4.10.0",
    "react": "^16.6.3",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.3.12",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tslint": "^5.11.0",
    "typedoc": "^0.13.0",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^3.2.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.10",
    "@fortawesome/free-solid-svg-icons": "^5.6.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@types/axios": "^0.14.0",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^4.1.4",
    "axios": "^0.18.0",
    "node-polyglot": "^2.3.0",
    "react-markdown": "^4.0.4",
    "react-polyglot": "^0.2.6",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.1.3"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "moduleNameMapper": {},
    "testResultsProcessor": "./node_modules/jest-html-reporter",
    "globals": {
      "NODE_ENV": "test"
    }
  },
  "jest-html-reporter": {
    "pageTitle": "Omigost Tests",
    "outputPath": "./docs/test-report.html",
    "includeFailureMsg": true
  }
}
